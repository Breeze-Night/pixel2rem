{
  "name": "Pixel to REM",
  "description": "Easily convert pixels to REM or vise-versa",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "img/maximize.png"
  },
  "icons": {
    "16": "img/maximize.png",
    "32": "img/maximize.png",
    "48": "img/maximize.png",
    "128": "img/maximize.png"
  }
}
